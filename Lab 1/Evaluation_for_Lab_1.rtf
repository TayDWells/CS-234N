SQL Code:\line /* 1 Select all of the customers who live in NY state. Show ID, Name, City, and State. Sort in a reasonable way. */\par
SELECT CustomerID, Name, City, State\par
FROM Customers\par
WHERE State = 'NY'\par
ORDER BY State, City, Name;\par
\par
/* 2 Select all of the states that start with A. Show both Code and Name. Sort by Name. */\par
SELECT State, CustomerID, Name\par
FROM Customers\par
WHERE State LIKE 'A%'\par
ORDER BY Name;\par
\par
/* 3 Select all of the products that have a price between 50 and 60 dollars. Show Code, Description, Price, and Quantity. Sort by Price. */\par
SELECT ProductCode, Description, UnitPrice, OnHandQuantity\par
FROM Products\par
WHERE UnitPrice BETWEEN 50 AND 60\par
ORDER BY UnitPrice;\par
\par
/* 4 Get the value of the inventory that we have on hand for each product. Show Code, Description, Price, Quantity, and Value. Sort by Value. */\par
SELECT ProductCode, Description, UnitPrice, OnHandQuantity * UnitPrice AS TotalValue\par
FROM Products\par
ORDER BY TotalValue;\par
\par
/* 5 Get me a list of the invoices. Show Invoice ID, Customer ID, Invoice Month as a number, Invoice Month as a word, and Invoice Year. Sort by Year and Month Number. */\par
SELECT InvoiceID, CustomerID, MONTH(InvoiceDate) AS MonthNumber, MONTHNAME(InvoiceDate) AS Month, YEAR(InvoiceDate)\par
FROM Invoices\par
ORDER BY YEAR(InvoiceDate), MONTH(InvoiceDate);\par
\par
/* 6 Get all of the information for the following products: A4CS, ADC4, CS10 */\par
SELECT *\par
FROM Products\par
WHERE ProductCode IN ('A4CS', 'ADC4', 'CS10');\par
\par
/* 7 Add the name of the state to the result set from #1 */\par
SELECT CustomerID, Name, City, State, StateName\par
FROM Customers\par
JOIN States ON Customers.State = States.StateCode\par
WHERE State = 'NY'\par
ORDER BY State, City, Name;\par
\par
/* 8 Add the customer's name to the result set from #5 */\par
SELECT I.InvoiceID, I.CustomerID, MONTH(InvoiceDate) AS MonthNumber, MONTHNAME(InvoiceDate) AS Month, YEAR(InvoiceDate), C.Name AS CustomerName\par
FROM Invoices AS I\par
JOIN Customers AS C ON I.CustomerID = C.CustomerID\par
ORDER BY YEAR(InvoiceDate), MONTH(InvoiceDate);\par
\par
/* 9 Get me a list of all of the products that have been ordered. Show all of the product information as well as the Invoice ID and the quantity ordered on each invoice. */\par
SELECT P.ProductCode, Description, P.UnitPrice, OnHandQuantity, InvoiceID, Quantity, IL.UnitPrice\par
FROM Products AS P\par
INNER JOIN InvoiceLineItems AS IL ON P.ProductCode = IL.ProductCode;\par
\par
/* 10 Get me a list of all of the invoices. Show Invoice ID, Customer ID, Invoice Date, as well as the Product Code, Description, Unit Price, and Quantity for each product ordered on the invoice. You'll have more than one record for each invoice. */\par
SELECT I.InvoiceID, I.CustomerID, I.InvoiceDate, ILI.ProductCode, P.Description, P.UnitPrice, ILI.Quantity\par
FROM Invoices AS I\par
JOIN InvoiceLineItems AS ILI ON I.InvoiceID = ILI.InvoiceID\par
JOIN Products AS P ON ILI.ProductCode = P.ProductCode;\par
\par
/* 11 Add the customer's name and state code to the results from #10. */\par
SELECT I.InvoiceID, I.CustomerID, I.InvoiceDate, ILI.ProductCode, P.Description, P.UnitPrice, ILI.Quantity, C.Name AS CustomerName, C.State\par
FROM Invoices AS I\par
JOIN InvoiceLineItems AS ILI ON I.InvoiceID = ILI.InvoiceID\par
JOIN Products AS P ON ILI.ProductCode = P.ProductCode\par
JOIN Customers AS C ON I.CustomerID = C.CustomerID;\par
\par
/* 12 Add the name of the state to the results from #11. */\par
SELECT I.InvoiceID, I.CustomerID, I.InvoiceDate, ILI.ProductCode, P.Description, P.UnitPrice, ILI.Quantity, C.Name AS CustomerName, C.State, S.StateName AS StateName\par
FROM Invoices AS I\par
JOIN InvoiceLineItems AS ILI ON I.InvoiceID = ILI.InvoiceID\par
JOIN Products AS P ON ILI.ProductCode = P.ProductCode\par
JOIN Customers AS C ON I.CustomerID = C.CustomerID\par
JOIN States AS S ON C.State = S.StateCode;\par
\par
/* 13 How many products do we have? */\par
SELECT COUNT(*)\par
FROM Products;\par
\par
/* 14 How many customers do we have? */\par
SELECT COUNT(*)\par
FROM Customers;\par
\par
/* 15 What's the total value of all of the products we have on hand? */\par
SELECT SUM(UnitPrice * OnHandQuantity) AS TotalValue\par
FROM Products;\par
\par
/* 16 What's the total value of ALL of the products we have sold? Use the ItemTotal from InvoiceLineItems to do the calculation. */\par
SELECT SUM(ItemTotal) AS TotalSold\par
FROM InvoiceLineItems;\par
\par
/* 17 What's the total value of EACH product we have sold? Use the ItemTotal from InvoiceLineItems to do the calculation. Show the Product Code as well as the total for that product. */\par
SELECT ProductCode, SUM(ItemTotal) AS TotalSold\par
FROM InvoiceLineItems\par
GROUP BY ProductCode\par
ORDER BY ProductCode;\par
\par
/* 18 Add the product description to #17. */\par
SELECT I.ProductCode, P.Description, SUM(ItemTotal) AS TotalSold\par
FROM InvoiceLineItems AS I\par
JOIN Products AS P ON I.ProductCode = P.ProductCode\par
GROUP BY I.ProductCode, P.Description\par
ORDER BY I.ProductCode;\par
\par
/* 19 How many orders (invoices) has each customer placed? Show CustomerID, Name, and count. */\par
SELECT C.CustomerID, C.Name AS CustomerName, COUNT(I.InvoiceID) AS NumberOfOrders\par
FROM Customers AS C\par
JOIN Invoices AS I ON C.CustomerID = I.CustomerID\par
GROUP BY C.CustomerID, C.Name\par
ORDER BY C.CustomerID;\par
\par
/* 20 List all customers, even if they don't have any orders. Show CustomerID, Name, and count of orders (invoices). */\par
SELECT C.CustomerID, C.Name AS CustomerName, COUNT(I.InvoiceID) AS NumberOfOrders\par
FROM Customers AS C\par
LEFT JOIN Invoices AS I ON C.CustomerID = I.CustomerID\par
GROUP BY C.CustomerID, C.Name\par
ORDER BY C.CustomerID;\par
\par
